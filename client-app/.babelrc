{
  "presets": ["react", "es2015", "stage-3"],
  "plugins": [
    "emotion",
    "syntax-async-functions",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "transform-es2015-spread"
  ],
  "env": {
    "development": {
      "presets": ["react-hmre"]
    }
  }
}
